{% extends 'base.html.twig' %}

{% block body %}

<h1>Create new product</h1>

{{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.description) }}
    {{ form_row(form.size) }}
    {{ form_row(form.save, {'label':'SAve'}) }}
{{ form_end(form) }}

<a href="{{ path('product_index') }}">Back</a>
{% endblock %}